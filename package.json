{
  "name": "logic-playground",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "main.ts",
  "scripts": {
    "build": "vinxi build",
    "start": "vinxi start",
    "dev": "npm run openapi:generate && vinxi dev",
    "db:pull": "drizzle-kit pull",
    "db:generate": "drizzle-kit generate",
    "db:up": "drizzle-kit migrate",
    "db:wipe": "docker-compose rm -s -v -f db && docker volume prune -f && docker-compose up -d db && sleep 10 && npm run db:up",
    "db:seed": "tsx ./scripts/seed/seed.ts",
    "lint": "eslint . --fix --ignore-pattern '.output/**'",
    "openapi:generate": "tsx ./scripts/openapi/generateSchema.ts && openapi-ts"
  },
  "author": "Alex Sarychev",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hey-api/client-axios": "^0.6.2",
    "@mui/material": "^6.4.8",
    "@tanstack/react-query": "^5.68.0",
    "@tanstack/react-router": "^1.114.22",
    "@tanstack/react-router-devtools": "^1.114.22",
    "@tanstack/react-start": "^1.114.22",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.7.0",
    "eslint-plugin-import": "^2.31.0",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vinxi": "0.5.3",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.1",
    "zod-openapi": "^4.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@hey-api/openapi-ts": "^0.64.12",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  }
}
