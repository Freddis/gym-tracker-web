{
  "name": "gym-tracker",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "start": "node ./.output/server/index.mjs",
    "dev": "npm run openapi:generate && vite dev",
    "dev:all": "concurrently --kill-others 'npm run dev -- --host' 'npm run storybook'",
    "check": "npm run typecheck && npm run lint ",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:browser": "npx playwright test --headed --workers 1",
    "test:gha": "npx vitest --run --coverage --reporter=verbose",
    "test:coverage": "npm run test -- --coverage && rm -R ./html/coverage/html && mv ./html/coverage/spa ./html/coverage/html && vite preview --outDir html --open",
    "db:backup": "mkdir ./temp/db_backup; PGPASSWORD=password pg_dump --clean -h 127.0.0.1 -p 15432 -U postgres -d gym > ./temp/db_backup/\"$(date +\"%F %T\").sql\" && echo 'done'",
    "db:restore": "PGPASSWORD=password psql -h 127.0.0.1 -p 15432 -U postgres -d gym -f ./temp/db_backup/\"$(ls -Art ./temp/db_backup | tail -n 1)\" && echo 'done'",
    "db:generate": "drizzle-kit generate",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "drizzle-kit migrate",
    "db:clean": "docker-compose rm -s -v -f db && docker volume prune -f && docker-compose up -d db && sleep 10 && npm run db:migrate",
    "db:seed": "tsx ./scripts/argus/seed.ts",
    "db:wipe": "npm run db:clean && npm run db:seed",
    "lint": "eslint . --fix --ignore-pattern '.output/**'",
    "openapi:generate": "tsx ./scripts/openapi/generateSchema.ts",
    "storybook": "storybook dev -c storybook  -p 6006 --no-open"
  },
  "author": "Alex Sarychev",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.848.0",
    "@hey-api/client-axios": "^0.6.2",
    "@hey-api/client-fetch": "^0.10.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-switch": "^1.2.5",
    "@tanstack/react-query": "^5.77.0",
    "@tanstack/react-router": "^1.121.34",
    "@tanstack/react-router-devtools": "^1.121.34",
    "@tanstack/react-start": "^1.121.35",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.7.0",
    "eslint-plugin-import": "^2.31.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "pg": "^8.13.1",
    "qs": "^6.14.0",
